                </xs:annotation>
            </xs:element>
            <xs:element name="actualCommitment" type="xs:double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The actual researcher commitment</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
